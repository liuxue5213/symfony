<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserBundle\Entity\JsAuthorityRoleMenu" table="js_authority_role_menu">
    <indexes>
      <index name="menuid" columns="menu_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="roleId" type="integer" column="role_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="menuId" type="integer" column="menu_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="showId" type="integer" column="show_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="status" type="integer" column="status" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
