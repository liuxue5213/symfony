<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserBundle\Entity\JsUser" table="js_user">
    <indexes>
      <index name="user_login_key" columns="user_loginame"/>
      <index name="user_nicename" columns="user_nickname"/>
    </indexes>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userLoginame" type="string" column="user_loginame" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userNickname" type="string" column="user_nickname" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userPass" type="string" column="user_pass" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userEmail" type="string" column="user_email" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userUrl" type="string" column="user_url" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userRegistered" type="datetime" column="user_registered" nullable="false"/>
    <field name="userStatus" type="integer" column="user_status" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userLastime" type="datetime" column="user_lastime" nullable="false"/>
    <field name="userRole" type="integer" column="user_role" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
