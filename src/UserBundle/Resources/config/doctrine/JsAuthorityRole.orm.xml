<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserBundle\Entity\JsAuthorityRole" table="js_authority_role">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pid" type="integer" column="pid" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="true"/>
    <field name="ctime" type="datetime" column="ctime" nullable="true"/>
    <field name="remark" type="string" column="remark" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="deleteFlg" type="boolean" column="delete_flg" nullable="true"/>
    <field name="isDelete" type="boolean" column="is_delete" nullable="true"/>
  </entity>
</doctrine-mapping>
