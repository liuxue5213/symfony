<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserBundle\Entity\JsAuthorityMenuMapping" table="js_authority_menu_mapping">
    <indexes>
      <index name="group_id" columns="role_id"/>
      <index name="menu_id" columns="menu_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="roleId" type="integer" column="role_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="menuId" type="integer" column="menu_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="state" type="string" column="state" length="112" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="deleteFlg" type="integer" column="delete_flg" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
