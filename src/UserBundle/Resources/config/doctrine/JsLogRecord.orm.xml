<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserBundle\Entity\JsLogRecord" table="js_log_record">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="operationInfo" type="string" column="operation_info" length="2000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="functionName" type="string" column="function_name" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="addtime" type="datetime" column="addtime" nullable="true"/>
    <field name="operationType" type="string" column="operation_type" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="operationName" type="string" column="operation_name" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="operationIp" type="string" column="operation_ip" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
