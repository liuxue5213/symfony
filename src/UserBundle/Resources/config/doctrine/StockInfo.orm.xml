<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserBundle\Entity\StockInfo" table="stock_info">
    <indexes>
      <index name="stock_name" columns="stock_name"/>
    </indexes>
    <id name="id" type="integer" column="id"/>
    <id name="stockNo" type="integer" column="stock_no"/>
    <field name="stockName" type="string" column="stock_name" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="stockIndustry" type="string" column="stock_industry" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="stockAddr" type="integer" column="stock_addr" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">0 上海  1深圳</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
